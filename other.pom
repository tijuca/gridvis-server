                        <groupId>org.codehaus.groovy.maven</groupId>
                        <artifactId>gmaven-plugin</artifactId>
                        <executions>
                            <execution>
                                <phase>install</phase>
                                <goals>
                                    <goal>execute</goal>
                                </goals>
                                <configuration>
                                    <verbose>true</verbose>
                                    <properties>
                                        <port>${nbtestcase.server.port}</port>
                                        <pomBase>${pom.basedir}</pomBase>
                                        <!-- inject via local settings.xml -->
                                        <!-- -->
                                        <qftestexe>${qftest.exe}</qftestexe>
                                        <qftestff>${qftest.ff}</qftestff>
                                        <qftestjdk>${qftest.jdk}</qftestjdk>
                                        <qftestsrc>${qftest.src}</qftestsrc>
                                        <qftestmaxheapsize>${qftest.server.maxheapsize}</qftestmaxheapsize>
                                    </properties>
                                    <source>
                                        println "Starting Server for QF-Test ..."
                                        // directory defs
                                        def buildDir = "${project.build.directory}"
                                        def serverDataDir = "${buildDir}/serverdata"
                                        def testDB = "${buildDir}/test-databases/JanServer"
                                        def branding = project.properties["branding-token"]

                                        // create serverdata dir, if not existant
                                        new File("${serverDataDir}").mkdir()
                                        //make sure no failed security test is left a security realm configured:
                                        new File("${buildDir}/${branding}/security.properties").delete();

                                        // Patch server.conf with serverdata dir and port
                                        def myFile = new File("${buildDir}/server/etc/server.conf")
                                        def fileText = myFile.text
                                        fileText = fileText.replace('${installer:PORT}', "${project.properties.port}")
                                        fileText = fileText.replace('${installer:serverDataDir}', "${serverDataDir}")
                                        fileText = fileText.replace('${installer:maxHeapSize}', "${project.properties.qftestmaxheapsize}")
                                        myFile.write(fileText)

                                        // Set Variables
                                        def LOGS = "${buildDir}/surefire-reports"
                                        def URL = "http://localhost:${project.properties.port}/"

                                        // Start gridvis-qftests setQuickGVE Test
                                        println "Starting QF-Test ..."
                                        def cmdQF = """${project.properties.qftestexe} -nomessagewindow -java ${project.properties.qftestjdk} -exitcodeignoreexception -batch -compact -runlog $LOGS/GVE/TEST-QF-Server-GVE -report.html $LOGS/GVE -report.junit $LOGS/GVE -variable errorReportDumpDir=${LOGS}/GVE -variable pathService=${buildDir}/${branding} -variable pathFirefox=${project.properties.qftestff} -variable coverage="false" -variable databases="0" -variable avoidInstall="true" ${project.properties.qftestsrc}/TestSets/setQuickGVE.qft"""
                                        Process QF = cmdQF.execute()
                                        QF.waitForProcessOutput System.out,System.err
                                        //quick test should be finished in 5minutes
                                        QF.waitForOrKill(5*60*1000)
                                        println "return code: ${ QF.exitValue()}"
                                        println "QF-Test finished ..."


                                        println "QF-Test finished ..."
                                    </source>
                                </configuration>
                            </execution>
                        </executions>
                    </plugin>
                </plugins>
            </build>
