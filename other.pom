        <netbeans.version.unit>${netbeans.version}</netbeans.version.unit>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <netbeans.conf.file>app.conf</netbeans.conf.file>
        <jenkins-creator-pin>gridvis-docker</jenkins-creator-pin>
        <selenium.hub>localhost</selenium.hub>
        <selenium.port>4444</selenium.port>
        <servlet.server>localhost</servlet.server>
        <jenkins-creator-package>default</jenkins-creator-package>
        <!-- Make sure we have the latest cluster versions. -->
        <sanity-force-cluster-version-on>.*</sanity-force-cluster-version-on>

        <userdirname>userdir</userdirname>
        <build.temp>${java.io.tmpdir}/${project.version}</build.temp>
        <netbeans.run.params.ide/>
        <!-- The run parameters are only for the IDE!. To change the parameters use the applications/*/app.conf file. -->

        <!-- activate own logback config via profile  -->
        <cli-own-logback-config/>
        <cli-jRebel/>
        <cli-xRebel/>
        <cli-logback>-J-Dnetbeans.mainclass=de.janitza.pasw.arg.catcher.MainClass -J-Djava.util.logging.config.class=de.janitza.pasw.pasw.logback.JulHandler ${cli-own-logback-config}</cli-logback>
        <cli-jvm>  -J-Xms64m -J-Xmx500m -J-Dvisualvm.display.name=${branding-token}  -J-ea ${cli-jRebel} ${cli-xRebel}</cli-jvm>
        <cli-netbeans>--branding ${branding-token} --locale de</cli-netbeans>
        <cli-gridvis>-J-Dfast-status-mail=true  -J-Dsharedplugin-live-reload=true</cli-gridvis>
        <netbeans.run.params>${netbeans.run.params.ide}  ${cli-logback} ${cli-jvm} ${cli-netbeans} ${cli-gridvis}</netbeans.run.params>
        <restapi.in.directory>${project.build.directory}/restapi-in</restapi.in.directory>
        <extraStartupJRebel/>
        <restapi.out.directory>${project.build.directory}/restapi-out</restapi.out.directory>
        <reportsBaseDir>${project.build.directory}/${branding-token}/reports</reportsBaseDir>
        <gridvis.languages>cs,de,en,es,fr,ja,ko,nl,pt,ru,sr,tr,zh_CN,zh_TW,nl_NL,pt_PT,it</gridvis.languages>
    </properties>

