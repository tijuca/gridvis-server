            we need to add something, so install and deploy won't fail -->
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>build-helper-maven-plugin</artifactId>
                <version>1.7</version>
                <executions>
                    <execution>
                        <id>verify01</id>
                        <phase>verify</phase>
                        <goals>
                            <goal>attach-artifact</goal>
                        </goals>
                        <configuration>
                            <artifacts>
                                <artifact>
                                    <file>
                                        pom.xml
                                    </file>
                                    <type>pom</type>
                                </artifact>
                            </artifacts>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <artifactId>maven-assembly-plugin</artifactId>
                <version>2.4</version>
                <executions>
                    <execution>
                        <id>deploy03</id>
                        <phase>deploy</phase>
                        <goals>
                            <goal>single</goal>
                        </goals>
                        <configuration>
                            <archiveBaseDirectory>${project.build.directory}/${brandingToken}</archiveBaseDirectory>
                            <outputDirectory>${project.build.directory}/dist</outputDirectory>
                            <tarLongFileMode>gnu</tarLongFileMode>
                            <descriptors>
                                <descriptor>src/main/assembly/preinstaller.xml</descriptor>
                            </descriptors>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
            <!-- Very Important -->
            <!-- We have to invoke the default deploy manually to ensure it is called after create-installer and attach-art.. -->
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-deploy-plugin</artifactId>
                <version>2.7</version>
                <executions>
                    <!-- ATTENTION: If Maven decides to bind the default-deploy execution to another id, we need
                    to modify the following execution entry, to disable the default-deploy execution -->
                    <execution>
                        <id>default-deploy</id>
                        <phase>none</phase>
                    </execution>
                    <execution>
                        <id>deploy04</id>
                        <phase>deploy</phase>
                        <goals>
                            <goal>deploy</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-surefire-plugin</artifactId>
                <version>2.10</version>
                <configuration>
                    <argLine>-Djava.io.tmpdir=${java.io.tmpdir}</argLine>
                    <!-- Exclude jar since the test uses the nbm/jars in the cluster -->
                    <!--<classpathDependencyScopeExclude>test</classpathDependencyScopeExclude>-->
                    <!-- We need the jar with scopt test for code completion -->
                    <!-- We can't use this jar, we use the wrapped jar in the cluster -->
                    <!-- Otherwise the jar can't be loaded by the Netbeans class loader
                                        <classpathDependencyExcludes>
                                            <classpathDependencyExcludes>de.janitza.pasw:wrapper-jersey-client</classpathDependencyExcludes>
                                        </classpathDependencyExcludes>
                    -->
                    <!-- We need to define all clusters -->
                    <systemPropertyVariables>
                        <reportsDirectory>${project.build.directory}/surefire-reports</reportsDirectory>
                        <serverPort>${nbtestcase.server.port}</serverPort>
                        <all.clusters>${project.build.directory}/${branding-token}</all.clusters>
                        <selenium.hub>${selenium.hub}</selenium.hub>
                        <selenium.port>${selenium.port}</selenium.port>
                        <servlet.server>${servlet.server}</servlet.server>
                        <browser>${selenium.browser}</browser>
                        <proj-dir>${project.build.directory}/test-databases</proj-dir>
                        <nbjunit.workdir>${java.io.tmpdir}/${branding-token}-${project.version}</nbjunit.workdir>
                        <restapi.in.directory>${restapi.in.directory}</restapi.in.directory>
                        <restapi.out.directory>${restapi.out.directory}</restapi.out.directory>
                    </systemPropertyVariables>
                    <!-- Do not change is to once, unless the multpile jmx registration is fixed -->
                    <forkMode>pertest</forkMode>

                </configuration>
            </plugin>
        </plugins>
    </build>

