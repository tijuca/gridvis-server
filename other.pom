                <artifactId>nbm-maven-plugin</artifactId>
                <extensions>true</extensions>
                <configuration>
                    <brandingToken>${branding-token}</brandingToken>
                    <cluster>${branding-token}</cluster>
                </configuration>
                <executions>
                    <execution>
                        <id>default-standalone-zip</id>
                        <phase>none</phase>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>de.janitza.maven</groupId>
                <artifactId>branding-plugin</artifactId>
                <version>1.5.0</version>
                <executions>
                    <execution>
                        <!-- We must be after cluster-app and before the integration tests -->
                        <phase>pre-integration-test</phase>
                        <goals>
                            <goal>update-properties</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-antrun-plugin</artifactId>
                <version>1.6</version>
                <executions>
                    <execution>
                        <phase>compile</phase>
                        <goals>
                            <goal>run</goal>
                        </goals>

                        <configuration>
                            <target>
                                <copy file="src/main/logback/logback.xml" overwrite="true" todir="${project.build.directory}/${branding-token}/" verbose="true"/>
                                <move flatten="true" todir="${reportsBaseDir}">
                                    <fileset dir="${reportsBaseDir}/logo/${branding-token}" includes="*.png"/>
                                </move>
                                <delete dir="${reportsBaseDir}/logo/"/>
                            </target>
                        </configuration>
                    </execution>
                    <execution>
                        <id>deploy02</id>
                        <phase>deploy</phase>
                        <configuration>
                            <tasks>
                                <copy file="${project.build.directory}/generated-resources/xml/xslt/changelog.html" overwrite="true" tofile="${project.build.directory}/${branding-token}/changelog.html" verbose="true"/>
                            </tasks>
                        </configuration>
                        <goals>
                            <goal>run</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-dependency-plugin</artifactId>
                <version>2.3</version>
                <executions>
                    <execution>
                        <id>copy</id>
                        <phase>process-resources</phase>
                        <goals>
                            <goal>unpack</goal>
                        </goals>
                        <configuration>
                            <artifactItems>
                                <artifactItem>
                                    <groupId>de.janitza.pasw</groupId>
                                    <artifactId>reports</artifactId>
                                    <type>jar</type>
                                    <overWrite>true</overWrite>
                                    <outputDirectory>${project.build.directory}/${branding-token}/reports</outputDirectory>
                                    <includes>*.jasper, **/*.jrtx, **/*.png</includes>
                                </artifactItem>
                            </artifactItems>
                        </configuration>
                    </execution>
